<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GovtJobQuest"
        tools:targetApi="31">
        <activity
            android:name=".ResultActivity"
            android:exported="false"
            android:theme="@style/AppSignUpTheme"/>
        <activity
            android:name=".subCatagory.GAwarenessActivity"
            android:exported="false" />
        <activity
            android:name=".subCatagory.GScienceActivity"
            android:exported="false" />
        <activity
            android:name=".subCatagory.ReasoningActivity"
            android:exported="false" />
        <activity
            android:name=".subCatagory.MathActivity"
            android:exported="false" />

        <activity
            android:name=".QuizActivity"
            android:exported="false"
            android:theme="@style/AppSignUpTheme" />

        <activity
            android:name=".ForgetPasswordActivity"
            android:exported="false"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppSignUpTheme" />
        <activity
            android:name=".SignUpActivity"
            android:exported="false"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppSignUpTheme" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:theme="@style/AppSplashTheme" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/AppSplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/AppSignUpTheme" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id_test"/>

    </application>

</manifest>